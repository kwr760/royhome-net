[ignore]
.*/node_modules/.*\.json$

[include]

[libs]
./flow-typed/.*

[lints]
all=warn
untyped-type-import=error
sketchy-null-bool=off

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.html
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='html' -> 'empty/string'

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
