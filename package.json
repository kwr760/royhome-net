  "scripts": {
    "start:dev": "RELEASE_ENV=dev nodemon src/server/index.js",
    "build:dev": "yarn clean && webpack --env dev --watch",
    "test": "yarn test:flow && yarn test:lint && yarn test:only",
    "test:flow": "flow",
    "test:lint": "eslint src",
    "test:only": "jest --colors",
    "start:prod": "RELEASE_ENV=prod babel-node src/server/index.js",
    "build:prod": "yarn clean && RELEASE_ENV=prod webpack",
    "release": "yarn install --production && yarn run build:prod && yarn run pm2:start",
    "pm2:start": "pm2 start \"RELEASE_ENV=prod babel-node src/server/index.js\"",
    "pm2:stop": "pm2 kill",
    "pm2:status": "pm2 status",
    "clean": "rimraf build log/*.log"
  },
