  "scripts": {
    "build": "yarn clean && yarn build:dev",
    "build:dev": "webpack --env dev --watch",
    "start": "nodemon --watch dist --exec \"node --inspect dist/server.bundle.js\"",
    "eslint": "eslint src",
    "test": "eslint src && jest --colors",
    "clean": "rimraf dist coverage log/*.log unit",
    "start:prod": "RELEASE_ENV=prod node dist/server.bundle.js",
    "build:prod": "webpack --env prod",
    "pm2:start": "RELEASE_ENV=prod pm2 start dist/server.bundle.js",
    "pm2:stop": "pm2 stop dist/server.bundle.js",
    "pm2:kill": "pm2 kill",
    "pm2:status": "pm2 status",
    "release": "yarn install --production && yarn run build:prod && yarn run pm2:start"
  },
